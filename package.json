    "3D",
    "ascii-cube",
    "react",
    "typescript",
    "javascript"
  ],
  "dependencies": {
    "canvas": "^2.11.0",
    "react": "^18.2.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.2.0",
