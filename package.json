{
  "name": "nbody-simulator-react-p5",
  "description": "A react component that simulates the n-body problem using p5.js",
  "homepage": "https://im-rises.github.io/cube-ascii-react/",
  "repository": "https://github.com/Im-Rises/cube-ascii-react",
  "version": "0.1.0",
  "private": false,
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "main": "dist/Components/",
  "files": [
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "keywords": [
